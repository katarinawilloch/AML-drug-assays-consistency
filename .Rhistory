make_bold_names <- function(mat, rc_fun, row=TRUE) {
bold_names <- rc_fun(mat)
ids <- rownames(mat) %>% match(rc_fun(mat))
if(row==FALSE){
ids <- colnames(mat) %>% match(rc_fun(mat))
}
ids %>%
walk(
function(i)
bold_names[i] <<-
bquote(bold(.(rc_fun(mat)[i]))) %>%
as.expression()
)
bold_names
}
