p <- ggplot(df_long, aes(x = Var2, y = Var1, fill = value)) +
geom_tile() +
scale_fill_manual(values = c("TRUE" = "red", "FALSE" = "white")) +
labs(x = "Columns", y = "Rows", fill = "Missing") +
